#}
modeldir <- '../stan_models/'
modelname <- paste(modeldir,model_name, sep = '')
model <- readChar(modelname,file.info(modelname)$size)
return(stan(model_code = model, data = GC_data, iter = niter, warmup = warmup, chains = chains))
}
